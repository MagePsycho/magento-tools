<?xml version="1.0"?>
<!--@docstring Crontab task@-->
<config>
  <crontab>
    <jobs>
      <%cronjob_name%>
        <schedule>
          <cron_expr>%frequency__m_h_dom_mon_dow%</cron_expr>
        </schedule>
        <run>
          <model>%cron_target%::%cron_method%</model>
        </run>
      </%cronjob_name%>
    </jobs>
  </crontab>
</config>
