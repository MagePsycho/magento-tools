<?xml version="1.0"?>
<!--@docstring Controller rewrite for single action (regexp based path)@-->
<config>
  <global>
    <rewrite>
      <%override_handle%>
        <from><![CDATA[#%from_path_preg%#]]></from>
        <to>%to_path%</to>
      </%override_handle%>
    </rewrite>
  </global>
</config>
