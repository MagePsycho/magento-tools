<?xml version="1.0"?>
<!--@docstring Controller rewrite for single action (regexp based path)@-->
<config>
  <global>
    <rewrite>
      <%module%_%action%>
        <from><!--[CDATA[#%from_path%#]]--></from>
        <to>%to_path%</to>
      </%module%_%action%>
    </rewrite>
  </global>
</config>
